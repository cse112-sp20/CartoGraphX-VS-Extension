{
	"name": "extension",
	"displayName": "ChartGraphX",
	"description": "Tracks data to be displayed on an interactive map of the source code. Visualization is aimed to make people understand the entire scope of their project and see what part of the project their team members are working on",
	"publisher": "Remote 13",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/main.js",
	"contributes": {
		"commands": [
			{
				"command": "chartGraphX.displayState",
				"title": "Display State"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"unit-test": "./node_modules/.bin/jest"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"jest": "^26.0.1",
		"mocha": "^7.1.2",
		"ts-sinon": "^1.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"firebase": "^7.14.3"
	}
}
